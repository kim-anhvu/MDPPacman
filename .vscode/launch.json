  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python Pacan",
      "type": "python",
      "request": "launch",
      "program": "${workspaceFolder}/pacman/pacman.py",
      "cwd": "${workspaceFolder}/pacman",
      "args": ["-p", "MDPAgent", "-l", "mediumClassic", "-n", "1", "-q"],
      "console": "integratedTerminal",
      "osx": {
        "pythonPath": "/usr/bin/python"
      }
    }
  ]
}
